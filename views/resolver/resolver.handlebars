<h2>Hello {{user.name}}</h2>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    {{#each complaints}}
      <tr>
        <td>{{complaintNo}}</td>
        <td>{{title}}</td>
        <td>{{status}}</td>
      </tr>
    {{else}}
      <tr>
        <td colspan="3">No complaints assigned to you</td>
      </tr>
    {{/each}}
  </tbody>
</table>
